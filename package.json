    "eslint": "6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^3.0.0",
    "react-app-rewired": "^2.1.4",
    "gh-pages": "^3.0.0",
    "worker-loader": "^2.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "env-cmd -f .env.development react-app-rewired start",
    "sit": "env-cmd -f .env.sit react-app-rewired start",
    "build": "env-cmd -f .env.development react-app-rewired build && cp build/index.html build/404.html",
    "build-sit": "env-cmd -f .env.sit react-app-rewired build && cp build/index.html build/404.html",
    "build-production": "env-cmd -f .env.production react-app-rewired build && cp build/index.html build/404.html",
    "build-uat": "env-cmd -f .env.uat react-app-rewired build && cp build/index.html build/404.html",
    "build-netlify": "rm -rf .netlify/build && mv build .netlify/build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "surge": "surge --project ./build --domain nea-staging.surge.sh",
    "surge-sit": "surge --project ./build --domain nea-sit.surge.sh",
    "surge-app": "surge --project ./build --domain nea-app.surge.sh",
    "deploy": "yarn build && yarn surge",
    "deploy-sit": "yarn build-sit && yarn surge-sit && yarn surge-app",
    "eslint": "eslint ./src/*.js ./src/**/*.js ./src/**/**/*.js ./src/**/**/**/*.js",
    "deploy-github": "gh-pages -b master -d build"
  },
  "browserslist": {
    "production": [
      "ie 11",
      ">0.1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "ie 11",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">= 8.x",
    "npm": "6.x"
  },
  "homepage": "http://steenify.github.io/nea"
}
